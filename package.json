{
	"name": "@alova/adapter-uniapp",
	"version": "1.0.0-apha.1",
	"description": "uniapp adapter for alova.js",
	"homepage": "https://alova.js.org",
	"main": "dist/alova-adapter-uniapp.umd.min.js",
	"jsnext:main": "dist/alova-adapter-uniapp.esm.js",
	"module": "dist/alova-adapter-uniapp.esm.js",
	"types": "typings/index.d.ts",
	"scripts": {
		"clean": "rimraf ./dist",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"build:esm": "cross-env NODE_ENV=development rollup -c ./config/rollup.config.esm.js",
		"build:umd": "cross-env NODE_ENV=development rollup -c ./config/rollup.config.umd.js",
		"build:umd.min": "cross-env NODE_ENV=production rollup -c ./config/rollup.config.umd.js",
		"build": "npm run clean && npm run build:esm && npm run build:umd && npm run build:umd.min",
		"release": "npm publish",
		"coveralls": ""
	},
	"publishConfig": {
		"access": "public"
	},
	"author": "JOU-amjs",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/alovajs/adapter-uniapp"
	},
	"bugs": {
		"url": "https://github.com/alovajs/adapter-uniapp/issues"
	},
	"files": [
		"dist",
		"typings/*.d.ts"
	],
	"devDependencies": {
		"@babel/core": "^7.18.2",
		"@babel/preset-env": "^7.18.2",
		"@dcloudio/types": "^3.2.11",
		"@rollup/plugin-commonjs": "^22.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@rollup/plugin-replace": "^4.0.0",
		"@testing-library/jest-dom": "^5.16.4",
		"@types/jest": "^27.5.1",
		"@typescript-eslint/eslint-plugin": "^1.4.2",
		"@typescript-eslint/parser": "^1.4.2",
		"alova": "^2.0.2",
		"babel-jest": "^29.2.2",
		"coveralls": "^3.1.1",
		"cross-env": "5.2.0",
		"eslint": "^5.15.1",
		"jest": "^29.2.2",
		"jest-environment-jsdom": "^29.2.2",
		"prettier": "^2.7.1",
		"rimraf": "2.6.2",
		"rollup": "^2.74.1",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript2": "^0.31.2",
		"ts-jest": "^29.0.3",
		"ts-jest-mock-import-meta": "^1.0.0",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.0",
		"typescript": "^4.7.2",
		"vue": "^3.2.45"
	}
}
