{
	"name": "@alova/adapter-uniapp",
	"version": "1.1.1",
	"description": "uniapp adapter for alova.js",
	"homepage": "https://alova.js.org",
	"main": "dist/alova-adapter-uniapp.umd.min.js",
	"jsnext:main": "dist/alova-adapter-uniapp.esm.js",
	"module": "dist/alova-adapter-uniapp.esm.js",
	"types": "typings/index.d.ts",
	"keywords": [
		"hooks",
		"vuejs",
		"typescript",
		"vue",
		"request",
		"vue3",
		"superagent",
		"alova",
		"uniapp"
	],
	"scripts": {
		"clean": "rimraf ./dist",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"lint": "eslint --ext .ts,.js src/**",
		"lint:fix": "eslint --ext .ts,.js src/** --fix",
		"build:esm": "cross-env NODE_ENV=development rollup -c ./config/rollup.config.esm.js",
		"build:umd": "cross-env NODE_ENV=development rollup -c ./config/rollup.config.umd.js",
		"build:umd.min": "cross-env NODE_ENV=production rollup -c ./config/rollup.config.umd.js",
		"build": "npm run clean && npm run build:esm && npm run build:umd && npm run build:umd.min",
		"release": "semantic-release",
		"coveralls": "npm run test:coverage && coveralls < coverage/lcov.info",
		"commit": "git add . && git-cz && git push"
	},
	"publishConfig": {
		"access": "public"
	},
	"author": "JOU-amjs",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/alovajs/adapter-uniapp.git"
	},
	"bugs": {
		"url": "https://github.com/alovajs/adapter-uniapp/issues"
	},
	"files": [
		"dist",
		"typings/*.d.ts"
	],
	"devDependencies": {
		"@alova/mock": "^1.1.1",
		"@babel/core": "^7.18.2",
		"@babel/preset-env": "^7.18.2",
		"@commitlint/config-conventional": "^17.4.4",
		"@dcloudio/types": "^3.2.11",
		"@rollup/plugin-commonjs": "^22.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@rollup/plugin-replace": "^4.0.0",
		"@semantic-release/git": "^10.0.1",
		"@testing-library/jest-dom": "^5.16.4",
		"@types/jest": "^29.4.0",
		"@typescript-eslint/eslint-plugin": "^5.54.0",
		"@typescript-eslint/parser": "^5.54.0",
		"alova": "^2.0.4",
		"babel-jest": "^29.2.2",
		"commitizen": "^4.3.0",
		"commitlint": "^17.5.1",
		"coveralls": "^3.1.1",
		"cross-env": "5.2.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.35.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"jest": "^29.4.3",
		"jest-environment-jsdom": "^29.2.2",
		"prettier": "^2.7.1",
		"rimraf": "2.6.2",
		"rollup": "^2.74.1",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript2": "^0.31.2",
		"semantic-release": "^21.0.1",
		"ts-jest": "^29.0.3",
		"ts-jest-mock-import-meta": "^1.0.0",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.0",
		"typescript": "^4.7.2",
		"vue": "^3.2.45"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
